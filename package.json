{
  "name": "demobot",
  "version": "1.0.0",
  "description": "Nako's Demo Bot",
  "main": "app.js",
  "repository": "https://github.com/nako-app/demo-bot",
  "dependencies": {
    "lambda-ssm-loader": "^1.0.0",
    "nako-server-sdk": "^0.0.21",
    "node-fetch": "^2.6.1",
    "typescript": "^4.1.5"
  },
  "author": "Nako",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "copy": "cpx \"./node_modules/**/*\" ./dist/node_modules && cpx package.json ./dist",
    "deploy": "npm run build && npm run copy && sam build && sam deploy --no-confirm-changeset",
    "lint": "prettier --config=.prettierrc \"**/*.(js|ts)\" --write --ignore-unknown",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "**/*.ts": "prettier --write --ignore-unknown"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.71",
    "@types/node": "^14.14.30",
    "cpx": "^1.5.0",
    "husky": "^5.1.1",
    "lint-staged": "^10.5.4",
    "pinst": "^2.1.6"
  }
}
